{
  "rewrites": [
    {
      "source": "/:path",
      "destination": "/:path.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/",
      "statusCode": 404,
      "has": [
        {
          "type": "header",
          "key": "x-vercel-error",
          "value": "true"
        }
      ]
    }
  ]
}